extends layout

block extraHeader
  title D&D Platform - Login
  script(src='/lib/jquery-2.1.1.min.js')
  script(src='/lib/alertify.min.js')

block content
  include _login
  include _signup

  script.
    if ('#{error}') {
      alertify.error("#{error}");
    }
    if ('#{success}') {
      alertify.success('#{success}');
    }
